<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/*</from-view-id>
        <navigation-case>
            <from-outcome>personalBondMain</from-outcome>
            <to-view-id>/bond/personalBondMain.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>issuePersonalBond</from-outcome>
            <to-view-id>/bond/issuePersonalBond1.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>frontUserExtractCash</from-outcome>
            <to-view-id>/frontUser/frontUserExtractCash.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>issuePersonalBond4</from-outcome>
            <to-view-id>/bond/issuePersonalBond4.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>issuePlatBond</from-outcome>
            <to-view-id>/platBond/issuePlatBond1.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>platBondManage</from-outcome>
            <to-view-id>/platBond/platBondManage.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>operation</from-outcome>
            <to-view-id>/backUserManage/operation.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>organization</from-outcome>
            <to-view-id>/backUserManage/organization.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>permission</from-outcome>
            <to-view-id>/backUserManage/permission.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>role</from-outcome>
            <to-view-id>/backUserManage/role.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>user</from-outcome>
            <to-view-id>/backUserManage/user.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>main</from-outcome>
            <to-view-id>/main.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>personalBondManage</from-outcome>
            <to-view-id>/bond/personalBondManage.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>frontUserView</from-outcome>
            <to-view-id>/frontUser/frontUserView.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>backUserMessage</from-outcome>
            <to-view-id>/backUserManage/backUserMessage.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
        <navigation-case>
            <from-outcome>personalBondTrash</from-outcome>
            <to-view-id>/bond/personalBondTrash.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
    
    <application>
        <action-listener>
            org.primefaces.application.DialogActionListener
        </action-listener>
        <navigation-handler>
            org.primefaces.application.DialogNavigationHandler
        </navigation-handler>
        <view-handler>
            org.primefaces.application.DialogViewHandler
        </view-handler>
    </application>
    <managed-bean>
        <managed-bean-name>nptpSessionBean</managed-bean-name>
        <managed-bean-class>com.oakeel.PtpSessionBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/frontUser/frontUserView.xhtml</from-view-id>
        <navigation-case>
            <from-action>frontUserAccount</from-action>
            <to-view-id>/frontUser/frontUserAccount.xhtml</to-view-id>
            <redirect/>
        </navigation-case>
    </navigation-rule>
</faces-config>
